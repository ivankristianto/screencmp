{
  "name": "@ivankristianto/screencmp",
  "version": "0.0.1",
  "description": "Grab screenshots from url(s) and compare it",
  "bin": {
    "screencmp": "bin/screencmp"
  },
  "main": "dist/cli.js",
  "scripts": {
    "build": "npm run clean && BABEL_ENV=commonjs babel src --out-dir dist",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run clean && BABEL_ENV=commonjs babel src --watch --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ivankristianto/screencmp.git"
  },
  "keywords": [
    "screenshot",
    "screengrab",
    "screen-compare"
  ],
  "author": "Ivan Kristianto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivankristianto/screencmp/issues"
  },
  "homepage": "https://github.com/ivankristianto/screencmp#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "puppeteer": "^22.6.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/runtime": "^7.24.1",
    "@wordpress/prettier-config": "^3.11.0",
    "core-js": "^3.36.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5"
  }
}
